        function sfsi_processfurther(ref) {
            var feed_id = 'N3ZZZ1ZVR2lCY1M2SlFOSzIvSzlXRHFHSmhCcjJFb0pmS3pyRzhUSkxJWmdTMS9pLzlmRE1WL05HTHN5a1NmWGxTeEt0WTRSc2dLVEJoMHh5K2IxdnZsWFV2ZGtIdll2cjdWK1BxV08zQ0hkVUwycjZVWmh2VkwyVURYNldBNFF8Vnl2S052RDZhdjJ3MU5zK1FyalpWVXlaMlBrOTY1WnFISnZ3M1ZiK1NWaz0=';
